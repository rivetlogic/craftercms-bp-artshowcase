			<title>Footer</title>
			<description></description>
			<defaultOpen>false</defaultOpen>
			<fields>
				<field>
					<type>label</type>
					<id>mapCoordinatesInstructions</id>
					<iceId></iceId>
					<title>Map Coordinates Instructions</title>
					<description></description>
					<defaultValue></defaultValue>
					<help></help>
					<properties>
						<property>
							<name>text</name>
							<value>To get a place's coordinates, you can right click any point in Google Maps and select &quot;What's here?&quot;</value>
							<type>string</type>
						</property>
					</properties>
					<constraints>
					</constraints>
				</field>
				<field>
					<type>label</type>
					<id>validLatitude</id>
					<iceId></iceId>
					<title>Valid Latitude</title>
					<description></description>
					<defaultValue></defaultValue>
					<help></help>
					<properties>
						<property>
							<name>text</name>
							<value>Valid latitude is between -85 and 85</value>
							<type>string</type>
						</property>
					</properties>
					<constraints>
					</constraints>
				</field>
				<field>
					<type>input</type>
					<id>locationLatitude</id>
					<iceId></iceId>
					<title>Location Latitude</title>
					<description></description>
					<defaultValue>0</defaultValue>
					<help></help>
					<properties>
						<property>
							<name>size</name>
							<value>50</value>
							<type>int</type>
						</property>
						<property>
							<name>maxlength</name>
							<value>50</value>
							<type>int</type>
						</property>
						<property>
							<name>readonly</name>
							<value></value>
							<type>boolean</type>
						</property>
						<property>
							<name>tokenize</name>
							<value>false</value>
							<type>boolean</type>
						</property>
					</properties>
					<constraints>
						<constraint>
							<name>required</name>
							<value><![CDATA[true]]></value>
							<type>boolean</type>
						</constraint>
						<constraint>
							<name>pattern</name>
							<value><![CDATA[-?[0-9]+(\.[0-9]+)?]]></value>
							<type>string</type>
						</constraint>
					</constraints>
				</field>
				<field>
					<type>label</type>
					<id>validLongitude</id>
					<iceId></iceId>
					<title>Valid Longitude</title>
					<description></description>
					<defaultValue></defaultValue>
					<help></help>
