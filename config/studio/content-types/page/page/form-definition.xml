			<title>Section</title>
			<description></description>
			<defaultOpen>false</defaultOpen>
			<fields>
				<field>
					<type>node-selector</type>
					<id>sectionComponents</id>
					<iceId>page-contents</iceId>
					<title>Section Components</title>
					<description></description>
					<defaultValue></defaultValue>
					<help></help>
